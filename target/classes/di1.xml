<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
<!-- 
	SamsungTV samsungtv = new com.kosta.mavenApp.section1.SamsungTV("DDD모델", 1000);
 -->
<bean id="samsungtv" class="com.kosta.mavenApp.section1.SamsungTV">
	<constructor-arg name="model" value="DDD모델"> </constructor-arg>
	<constructor-arg name="price" value="1000"> </constructor-arg>
</bean>
<!-- default 생성자로 생성됨 
	LGTV lgtv = new com.kosta.mavenApp.section1.LGTV();
-->
<bean id="lgtv" class="com.kosta.mavenApp.section1.LGTV">

</bean>
</beans>
